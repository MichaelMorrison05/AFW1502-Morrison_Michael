ui=require("UI"),weatherApi=function(e,t){var a=Ti.Network.createHTTPClient({onload:function(){var e=JSON.parse(this.responseText),t={icon:e.current_observation.icon_url,city:e.current_observation.display_location.city,state:e.location.state,time:e.current_observation.local_time_rfc822,weather:e.current_observation.weather,lat:e.current_observation.display_location.latitude,lon:e.current_observation.display_location.longitude,temp_f:e.current_observation.temp_f,temp_c:e.current_observation.temp_c,webcam:e.webcams[8].CURRENTIMAGEURL,logo:"http://icons.wxug.com/logos/PNG/wundergroundLogo_4c.png",lastUpdate:e.current_observation.observation_time};ui.buildUI(t)},onerror:function(){alert("Error")}});a.timeout=5e3;var o="http://api.wunderground.com/api/d8d6d376722c1591/geolookup/conditions/forcast/webcams/astronomy/q/"+e+","+t+".json";a.open("GET",o),a.send()},exports.weather=weatherApi;
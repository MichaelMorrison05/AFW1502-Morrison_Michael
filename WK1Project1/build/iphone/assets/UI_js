var mainWindow,mainView,title,icon,logo,weather,temp_f,temp_c,city,time,lastUpdate,lat,lon,state,webcam;mainWindow=Ti.UI.createWindow({backgroundColor:"#1199ff"}),mainView=Ti.UI.createScrollView({layout:"vertical"}),title=Ti.UI.createLabel({color:"#fff",text:"My Weather App",font:{fontSize:22,fontFamily:"roboto",fontWeight:"bold"},top:50}),icon=Ti.UI.createImageView({top:20}),city=Ti.UI.createLabel({color:"#fff",top:20}),state=Ti.UI.createLabel({color:"#fff",top:1}),time=Ti.UI.createLabel({color:"#fff",top:1}),weather=Ti.UI.createLabel({color:"#fff",top:1}),lat=Ti.UI.createLabel({color:"#fff",top:1}),lon=Ti.UI.createLabel({color:"#fff",top:1}),temp_f=Ti.UI.createLabel({color:"#fff",top:1}),temp_c=Ti.UI.createLabel({color:"#fff",top:1}),webcam=Ti.UI.createImageView({top:50,height:240,width:352,color:"#fff"}),logo=Ti.UI.createImageView({height:156,width:210,top:180}),lastUpdate=Ti.UI.createLabel({color:"#fff",top:1}),exports.buildUI=function(e){console.log("obj: "+JSON.stringify(e)),icon.image=e.icon,logo.image=e.logo,temp_f.text="Fahrenheit: "+e.temp_f,temp_c.text="Celsius: "+e.temp_c,weather.text="Current weather: "+e.weather,city.text="City: "+e.city,time.text=e.time,lastUpdate.text=e.lastUpdate,lat.text="Latitude: "+e.lat,lon.text="Longitude: "+e.lon,state.text="State: "+e.state,webcam.image=e.webcam},mainView.add(title),mainView.add(icon),mainView.add(city),mainView.add(state),mainView.add(time),mainView.add(weather),mainView.add(lat),mainView.add(lon),mainView.add(temp_f),mainView.add(temp_c),mainView.add(webcam),mainView.add(logo),mainView.add(lastUpdate),mainWindow.add(mainView),mainWindow.open();